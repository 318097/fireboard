{
  "name": "Dot",
  "version": "1.0",
  "description": "Todos for development mode",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "content_scripts": [
    {
      "matches": ["http://localhost/*"],
      "js": ["./build/ext/script.js"]
    }
  ]
}
